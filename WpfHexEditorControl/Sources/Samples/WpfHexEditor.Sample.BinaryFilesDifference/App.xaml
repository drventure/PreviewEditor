<Application
    x:Class="WpfHexEditor.Sample.BinaryFilesDifference.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="clr-namespace:WpfHexaEditor.Core.Converters;assembly=WPFHexaEditor"
    xmlns:local="clr-namespace:WpfHexEditor.Sample.BinaryFilesDifference"
    StartupUri="MainWindow.xaml">


    <Application.Resources>
        <conv:ByteToHexStringConverter x:Key="ByteHexStringConverter" Show0xTag="False" />
        <conv:LongToHexStringConverter x:Key="LongHexStringConverter" />
    </Application.Resources>


</Application>
